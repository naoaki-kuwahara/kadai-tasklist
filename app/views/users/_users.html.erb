<%= render 'users', users: @users %>
<% if users.any? %>
  <ul class="list-unstyled">
    <% users.each do |user| %>
      <li>
        <div>
          <div>
            <%= user.name %>
          </div>
          <div>
            <p><%= link_to 'View profile', user_path(user) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
<% end %>